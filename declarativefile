pipeline {
  agent {label 'JENKINS'}
stages('git clone') {
  stage {
    steps {
      git url: 'https://github.com/99876543/spring-petclinic.git'
          branch: 'declarative'
    }
  }
}
  stage('Build') {
    steps {
      sh './mvnw clean package'
    }
  }
  stage('Deploy') {
    steps {
       sh './mvnw deploy'
    }
  }
}
}
