pipeline {
    agent any
    stages {
      stage('clone code') {
       steps {
          git url: 'https://github.com/99876543/spring-petclinic.git',
             branch: 'declarative'
        }
      }
      stage('build') {
         steps { 
           sh './mvnw package'
        }       
      }
   }
}
   
