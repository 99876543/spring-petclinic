pipeline {
    agent { label gradleFile }
    stages {
      stage('clone code') {
       steps {
          git url: 'https://github.com/99876543/spring-petclinic.git',
             branch: 'gradleFile'
        }
      }
      stage('build') {
         steps { 
           sh './gradle build'
        }       
      }
   }
}
